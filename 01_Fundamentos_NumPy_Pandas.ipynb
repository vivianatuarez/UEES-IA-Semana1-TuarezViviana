import numpy as np
import pandas as pd

# --- NumPy ---
arr = np.random.randint(1, 100, size=(5,5))
print("Array 5x5 aleatorio:\n", arr)

print("Suma por columnas:", arr.sum(axis=0))
print("Media total:", arr.mean())

